container_commands:
  01_install_wkhtmltopdf:
      command: sudo yum -y install fontconfig libXrender libXext xorg-x11-fonts-Type1 xorg-x11-fonts-75dpi freetype libpng zlib libjpeg-turbo openssl icu
      ignoreErrors: true
  02_install_wkhtmltopdf:
      command: wget https://github.com/wkhtmltopdf/packaging/releases/download/0.12.6-1/wkhtmltox-0.12.6-1.amazonlinux2.x86_64.rpm
      ignoreErrors: true
  03_install_wkhtmltopdf:
      command: sudo yum -y install wkhtmltox-0.12.6-1.amazonlinux2.x86_64.rpm
      ignoreErrors: true

